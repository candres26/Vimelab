{% extends "ScontrolBundle::layout.html.twig" %}

{% block title %}Descripción De Grupo De Patología{% endblock %}

{% block area %}

<h2 align="left">Descripción De Grupo De Patología</h2>

<table class="vime-table-des" align="center">
    <tbody>
        <tr>
            <th align="left">Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th align="left">Nombre</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th align="left">Alternativo</th>
            <td>{{ entity.alternativo }}</td>
        </tr>
    </tbody>
</table>

    <form id="vime-form" action="{{ path('mdgrup_delete', { 'id': entity.id }) }}" method="post">
        {{ form_widget(delete_form) }}
    </form>
    
{% endblock %}

{% block action %}
<div class="vime-action">
    <table>
        <tr>
            <td>&nbsp;&nbsp;&nbsp;</td>
            <td onClick="go('{{ path('mdgrup') }}')"><img src="{{ asset('bundles/scontrol/imagenes/iconos/listar.png') }}" title="Listar"></td>
            <td onClick="gId('vime-form').submit()"><img src="{{ asset('bundles/scontrol/imagenes/iconos/eliminar.png') }}" title="Eliminar"></td>
            <td onClick="go('{{ path('mdgrup_edit', { 'id': entity.id }) }}')"><img src="{{ asset('bundles/scontrol/imagenes/iconos/editar.png') }}" title="Editar"></td>
        </tr>
    </table>
</div>
{% endblock %}
